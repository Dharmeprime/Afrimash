<script type="rocketlazyloadscript">window.addEventListener('DOMContentLoaded', function() {
        jQuery(document).ready(function () {
            if (jQuery('body').hasClass('search-results') || jQuery('body').hasClass('search-no-results')) {
                var params = {
                    event_name: "search",
                    keywords: "",
                    result_count: "1"
                };
                callDEngage(params);
            }

            jQuery("body").on("keyup",".dengage-newsletter .dengage-newsletter-email",function(event) {
                if (event.keyCode === 13) {
                    jQuery(this).parents(".dengage-newsletter").find(".dengage-newsletter-submit").click();
                    jQuery(this).focusout();
                }
            });
            jQuery("body").on("click", ".dengage-newsletter .dengage-newsletter-submit", function(e) {
                e.preventDefault();
                
                var dengageShowNewletterError = function(err) {
                    elParent.find(".dengage-newsletter-error label").html(err);
                    elParent.find(".dengage-newsletter-error").fadeIn();
                }
                var dengageHideNewletterError = function(err) {
                    elParent.find(".dengage-newsletter-error").hide();
                }

                var btnSubmit = jQuery(this);
                if(btnSubmit.is(":disabled")) {
                    return;
                }
                var elParent = btnSubmit.parents(".dengage-newsletter");
                var txtEmail = elParent.find(".dengage-newsletter-email");
                var email = txtEmail.val()
                if((email || "") == "") {
                    txtEmail.addClass("dengage-required");
                    txtEmail.focus();
                    dengageShowNewletterError("Email is mandatory.");
                    return;
                } else if (!validateEmail(email)) {
                    txtEmail.addClass("dengage-required");
                    txtEmail.focus();
                    dengageShowNewletterError("Email is not valid.");
                    return;
                } 
                txtEmail.removeClass("dengage-required");
                btnSubmit.attr("disabled", true).val("Loading...");
                
                jQuery.ajax({
                    type: "post",
                    dataType: "json",
                    url: 'https://afrimash.com/wp-admin/admin-ajax.php',
                    data: {
                        action: "dengage_newsletter_submit",
                        email: email
                    },
                    success: function (response) {
                        btnSubmit.attr("disabled", false).val("Submit");
                        if (response.type == "success") {
                            txtEmail.val("");
                            elParent.find(".dengage-newsletter-form").hide();
                            elParent.find(".dengage-newsletter-thankyou-label").fadeIn();
                        } else {
                            dengageShowNewletterError(response.msg);
                        }
                    },
                    error: function (xhr, error) {
                        btnSubmit.attr("disabled", false).val("Submit");
                        dengageShowNewletterError("There seems to be an unexpected error.");
                        console.debug("xhr -> ", xhr); 
                        console.debug("error -> ", error);
                    }
                });
            });
        });
        function validateEmail(email) {
            const re = /^(([^<>()[\]\\.,;:\s@\"]+(\.[^<>()[\]\\.,;:\s@\"]+)*)|(\".+\"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
            return re.test(email);
        }
    });</script>
